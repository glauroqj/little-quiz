{
  "name": "little-quiz",
  "version": "0.1.0",
  "private": true,
  "author": "Glauro Q Juliani",
  "scripts": {
    "dev": "react-scripts start src && cra-append-sw --mode dev ./public/service-worker-custom.js",
    "build": "react-scripts build src && cra-append-sw --skip-compile ./public/service-worker-custom.js",
    "deploy": "yarn build && firebase deploy"
  },
  "dependencies": {
    "cra-append-sw": "^2.6.1",
    "date-fns": "^1.30.1",
    "firebase": "^6.3.5",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.0",
    "react-toastify": "^5.3.2",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "node-sass": "^7.0.0",
    "redux-devtools-extension": "^2.13.8"
  }
}
